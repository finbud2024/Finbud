    "netlify:build": "cd frontend && vue-cli-service build",
    "client": "cd frontend && vue-cli-service serve",
    "client:dev": "cd frontend && vue-cli-service serve --skip-plugins @vue/cli-plugin-eslint",
    "start": "nodemon backend/functions/server.mjs",
    "dev": "dotenv -e .env netlify dev",
    "seeding": "node backend/utils/stockSeeding.js",
    "install:python-deps": "pip install -r backend/functions/requirements.txt",
    "install:client": "cd frontend && npm install",
    "postinstall": "npm run install:client",
    "optimize": "node optimize-build.mjs",
    "clean": "rm -rf node_modules/.cache frontend/node_modules/.cache .cache frontend/.cache && npm run optimize",
    "analyze-bundle": "cd frontend && npx webpack-bundle-analyzer dist/js/*.js"
  },
